{
  "expo": {
    "name": "apiFun",
    "description": "An app where I use an api call to get random pets. A user just refreshes by pulling down on the screen and a new pet arrives",
    "slug": "apiFun",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/paw.png",
    "splash": {
      "image": "./assets/paw.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
